{
  "compilerOptions": {
    "strict": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "allowJs": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "../node_modules",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "dom", "es5", "es6"],
    "types": [
      "cypress",
      "node",
      "cypress-wait-until",
      "@cypress/grep",
      "cypress-react-selector",
      "cypress-plugin-api",
      "cypress-real-events",
      "@4tw/cypress-drag-drop",
      "@types/papaparse",
      "cypress-qase-reporter"
    ],
    "typeRoots": ["../node_modules/@types"],
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules", "cache"]
}
